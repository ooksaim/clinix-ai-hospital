<svg width="800" height="450" viewBox="0 0 800 450" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background with hospital image effect -->
  <rect width="800" height="450" fill="#1E293B"/>
  <rect x="0" y="0" width="800" height="450" fill="url(#hospitalOverlay)" fill-opacity="0.6"/>
  
  <!-- Dashboard UI Elements -->
  <g opacity="0.9">
    <rect x="100" y="100" width="600" height="250" rx="8" fill="#FFFFFF"/>
    <rect x="120" y="120" width="200" height="30" rx="4" fill="#2563EB"/>
    
    <!-- Chart -->
    <rect x="120" y="170" width="250" height="150" rx="4" fill="#F8FAFC"/>
    <path d="M130 280L150 260L170 270L190 240L210 250L230 220L250 210L270 230L290 200L310 180L330 190L350 170" stroke="#2563EB" stroke-width="2"/>
    
    <!-- Stats -->
    <rect x="390" y="170" width="290" height="150" rx="4" fill="#F8FAFC"/>
    <rect x="410" y="190" width="100" height="15" rx="2" fill="#94A3B8"/>
    <rect x="410" y="220" width="250" height="20" rx="4" fill="#DBEAFE"/>
    <rect x="410" y="260" width="100" height="15" rx="2" fill="#94A3B8"/>
    <rect x="410" y="290" width="180" height="20" rx="4" fill="#DCFCE7"/>
  </g>
  
  <!-- Play Button -->
  <circle cx="400" cy="225" r="50" fill="#2563EB" fill-opacity="0.9"/>
  <path d="M385 200L425 225L385 250V200Z" fill="white"/>
  
  <!-- Title Overlay -->
  <rect x="0" y="350" width="800" height="100" fill="url(#gradientOverlay)"/>
  <text x="400" y="400" font-family="Arial" font-weight="bold" font-size="24" fill="white" text-anchor="middle">Clinix AI: Transforming Hospital Management</text>
  
  <!-- Patterns and Gradients -->
  <defs>
    <pattern id="hospitalOverlay" patternUnits="userSpaceOnUse" width="800" height="450">
      <rect width="800" height="450" fill="#1E293B"/>
      <!-- Hospital Building Silhouette -->
      <rect x="100" y="150" width="600" height="300" fill="#0F172A"/>
      <rect x="150" y="200" width="50" height="50" fill="#1E293B"/>
      <rect x="250" y="200" width="50" height="50" fill="#1E293B"/>
      <rect x="350" y="200" width="50" height="50" fill="#1E293B"/>
      <rect x="450" y="200" width="50" height="50" fill="#1E293B"/>
      <rect x="550" y="200" width="50" height="50" fill="#1E293B"/>
      <rect x="150" y="300" width="50" height="50" fill="#1E293B"/>
      <rect x="250" y="300" width="50" height="50" fill="#1E293B"/>
      <rect x="350" y="300" width="100" height="100" fill="#1E293B"/>
      <rect x="550" y="300" width="50" height="50" fill="#1E293B"/>
    </pattern>
    
    <linearGradient id="gradientOverlay" x1="0" y1="350" x2="0" y2="450">
      <stop offset="0%" stop-color="rgba(15, 23, 42, 0)"/>
      <stop offset="100%" stop-color="rgba(15, 23, 42, 1)"/>
    </linearGradient>
  </defs>
</svg>
